!!!
%html
  %head
    %title Hot Or Not: Home Edition
    = stylesheet_link_tag    'application', media: 'all'
    = stylesheet_link_tag    'animate', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}/
    %meta{:content => "Hot or Not Nashville Real Estate Edition", :name => "description"}/
    %meta{:content => "Nashville,Real Estate,Listings,Web Developer,Game", :name => "keywords"}/
    %meta{:content => "Matt Spell", :name => "author"}/
    %meta{:content => "IE=edge", "http-equiv" => "X-UA-Compatible"}/
  %body
    = render "layouts/header"

    = notice
    = alert
    = yield
